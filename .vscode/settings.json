{
    "C_Cpp.clang_format_fallbackStyle": "WebKit",
    "files.exclude": {
        "unversioned/": true,
        
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,

        "obj/": true,
        "libs/": false
    },
    "files.associations": {
        "*.h": "c",
        "*.c_": "c"
    }
}